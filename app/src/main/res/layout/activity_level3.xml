<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ericsauber.buttonclickdemo.Level3">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="1162dp"
        android:layout_height="1148dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.001"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.538"
        app:srcCompat="@drawable/w" />

    <TextView
        android:id="@+id/correct3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:textColor="#FF0000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/back3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:onClick="gotoMain"
        android:text="back"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.837"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/pa3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:onClick="level3"
        android:text="play again?"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/next3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:onClick="gotoGameActivity2"
        android:text="Next"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="49dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="291dp"
        tools:layout_editor_absoluteY="401dp" />

    <TextView
        android:id="@+id/progress3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="93dp"
        android:layout_marginRight="35dp"
        android:text="0/20"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button3_1"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:text="2"
        android:onClick="two"
        app:layout_constraintTop_toTopOf="@+id/button31"
        android:layout_marginTop="47dp"
        android:layout_marginLeft="49dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button27"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:text="3"
        android:onClick="three"
        android:layout_marginTop="47dp"
        app:layout_constraintTop_toBottomOf="@+id/button31"
        android:layout_marginLeft="49dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button28"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:text="4"
        android:onClick="four"
        app:layout_constraintTop_toTopOf="@+id/button27"
        android:layout_marginTop="44dp"
        android:layout_marginLeft="49dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button29"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="47dp"
        android:background="?attr/colorPrimary"
        android:onClick="five"
        android:text="5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button28" />

    <Button
        android:id="@+id/button30"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:text="6"
        android:onClick="six"
        app:layout_constraintTop_toTopOf="@+id/button29"
        android:layout_marginTop="47dp"
        android:layout_marginLeft="49dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/Button3_b"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:text="B"
        android:onClick="b"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button3_a"
        android:layout_marginTop="47dp" />

    <Button
        android:id="@+id/button3_c"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:layout_marginTop="8dp"
        android:onClick="c"
        android:text="C"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3_a"
        app:layout_constraintVertical_bias="0.134" />

    <Button
        android:id="@+id/button3_d"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:text="D"
        android:onClick="d"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button3_c"
        android:layout_marginTop="47dp" />

    <Button
        android:id="@+id/button3_F"
        android:layout_width="84dp"
        android:layout_height="49dp"
        android:text="False"
        android:onClick="fals"
        app:layout_constraintHorizontal_bias="0.643"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="38dp" />

    <Button
        android:id="@+id/button3_T"
        android:layout_width="84dp"
        android:layout_height="49dp"
        android:text="True"
        android:onClick="tru"
        app:layout_constraintHorizontal_bias="0.363"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="38dp" />

    <Button
        android:id="@+id/button3_a"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:text="A"
        android:onClick="a"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="141dp" />

    <Button
        android:id="@+id/button31"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:text="1"
        android:onClick="one"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="94dp"
        android:layout_marginLeft="49dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button3_paper"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="27dp"
        android:text="Paper"
        android:onClick="paper"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="92dp"
        app:layout_constraintTop_toTopOf="@+id/button3_rock" />

    <Button
        android:id="@+id/button3_scissor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="27dp"
        android:text="Scissor"
        android:onClick="scissor"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="46dp"
        app:layout_constraintTop_toBottomOf="@+id/button3_paper" />

    <Button
        android:id="@+id/button3_rock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="27dp"
        android:text="Rock"
        android:onClick="rock"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="114dp" />
</android.support.constraint.ConstraintLayout>
